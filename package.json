  "license": "MIT",
  "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
  "main": "./src/africa.js",
  "typings": "./typings/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://zestoy@github.com/zestoy/africa.git"
  },
